  set (sawTextToSpeech_INCLUDE_DIR "@sawTextToSpeech_INCLUDE_DIR@")
  set (sawTextToSpeech_LIBRARY_DIR "@sawTextToSpeech_LIBRARY_DIR@")
else ()
  # try to find the install dir, we know the install is using
  # share/sawTextToSpeech so we can go ../..
  set (RELATIVE_INSTALL_DIR "${THIS_FILE_PATH}/../..")
  get_filename_component (ABSOLUTE_INSTALL_DIR "${RELATIVE_INSTALL_DIR}" ABSOLUTE)
  # set directories using the install dir
  set (sawTextToSpeech_INCLUDE_DIR "${ABSOLUTE_INSTALL_DIR}/include")
  set (sawTextToSpeech_LIBRARY_DIR "${ABSOLUTE_INSTALL_DIR}/lib")
endif ()

